proc markline {cntxt x1 y1 x2 y2 fill tag} {
	scalecreate $cntxt line [concat $x1 $y1 $x2 $y2] -fill $fill -tag $tag
}
proc marksat {cntxt x y sat} {
	set tagname sat${sat}_marker

	markline $cntxt [expr {$x - 8}] $y [expr {$x - 1}] $y red $tagname
	markline $cntxt [expr {$x + 1}] $y [expr {$x + 8}] $y red $tagname
	markline $cntxt $x [expr {$y - 8}] $x [expr {$y - 1}] red $tagname
	markline $cntxt $x [expr {$y + 1}] $x [expr {$y + 8}] red $tagname

	scalecreate $cntxt text [concat [expr {$x + 1}] [expr {$y + 1}]] \
		-anchor sw -text $sat -tags $tagname

	return $tagname
}

proc movesat {cntxt x y sat} {
	set tagname sat${sat}_marker

	[lindex $cntxt 0] delete $tagname
	marksat $cntxt $x $y $sat
}

proc satpath {cntxt sat path} {
	set tagname sat${sat}_path

	# Use full lat, lng range for wrapping even if context
	# is for a smaller region
	set corners {-180 -90 180 90}

	set segs [wrappath $corners $path]
	scalecreate $cntxt line [lindex $segs 0] -tags $tagname
	while {[llength [lindex $segs 1]] > 0} {
		set segs [wrappath $corners [lindex $segs 1]]
		scalecreate $cntxt line [lindex $segs 0] -tags $tagname
	}
}

proc movesatpath {cntxt sat $path} {
	set tagname sat${sat}_path

	[lindex $cntxt 0] delete $tagname
	satpath $cntxt $sat $path
}
